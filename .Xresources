!Xcursor.theme: cursor-theme
Xcursor.size: 16

! Colors - colors everywhere!
rofi.color-enabled:                  true
rofi.color-normal:                   argb:00000000, #1aa, argb:11FFFFFF, #1aa,#333
rofi.color-urgent:                   argb:00000000, #f99, argb:11FFFFFF, #f99,#333
rofi.color-active:                   argb:00000000, #aa1, argb:11FFFFFF, #aa1,#333
rofi.color-window:                   argb:ee333333, #1aa,#1aa
rofi.separator-style:                dash
rofi.width: 600


!!*fading: 40
*fadeColor: #002b36

! URxvt General

URxvt.scrollBar: false
URxvt.internalBorder: 10
UXTerm*bellIsUrgent: true
URxvt.font: xft:Fira Mono:pixelsize=12,style=regular

URxvt.colorUL: #4682B4

URxvt.cursorBlink: false
URxvt.cursorUnderline: true

URxvt*scrollTtyOutput: false
URxvt*scrollWithBuffer: true
URxvt*scrollTtyKeypress: trueD

urxvt*depth: 32
urxvt*background: rgba:0000/0000/0200/c800

! special
*.foreground:   #d9d8d8
*.background:   #252b2e
*.cursorColor:  #d9d8d8

! black
*.color0:       #231f20
*.color8:       #737171

! red
*.color1:       #e06c75
*.color9:       #e06c75

! green
*.color2:       #99c794
*.color10:      #99c794

! yellow
*.color3:       #fac863
*.color11:      #fac863

! blue
*.color4:       #6699cc
*.color12:      #6699cc

! magenta
*.color5:       #9e6ffe
*.color13:      #9e6ffe

! cyan
*.color6:       #85cebc
*.color14:      #85cebc

! white
*.color7:       #d9d8d8
*.color15:      #ffffff

